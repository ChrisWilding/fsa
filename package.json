{
  "name": "fsa",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@gitlab.com:ChrisWilding/fsa.git",
  "author": "Chris Wilding <hello@chriswilding.co.uk>",
  "license": "Apache-2.0",
  "private": true,
  "dependencies": {
    "es6-promise": "^4.2.4",
    "isomorphic-fetch": "^2.2.1",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "jest-fetch-mock": "^1.5.0",
    "lint-staged": "^7.0.4",
    "prettier": "^1.12.0",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3"
  },
  "scripts": {
    "build": "webpack --mode production",
    "lint": "eslint src test webpack.config.babel.js --ext .js,.jsx",
    "precommit": "lint-staged",
    "prettier": "prettier --list-different .babelrc .eslintrc webpack.config.babel.js \"{src,test}/**/*.{js,json,jsx}\"",
    "start": "webpack-dev-server --mode development",
    "test": "yarn lint && yarn prettier && jest",
    "test:coverage": "yarn test --coverage",
    "test:watch": "yarn test --watch"
  },
  "jest": {
    "automock": false,
    "setupTestFrameworkScriptFile": "<rootDir>test/setup.js"
  },
  "lint-staged": {
    "{src,test}/**/*.{js,json,jsx}": [
      "prettier --write",
      "git add"
    ]
  }
}
